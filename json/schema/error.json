{
    "id": "http://vamp-plugins.org/piper/json/schema/error#",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "schema for an error object in a response to an rpcrequest",
    "type": "object",
    "properties": {
	"code": {
            "type": "number"
        },
        "message": {
            "type": "string"
        }
    },
    "required": [ "code", "message" ]
}

